<style>
  #suggestions {
      position: absolute;
      z-index: 1000; /* Ensure it appears above other elements */
    
  }
</style>

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title card_title_cus">Society Election Held Status
              <div class="table_title_icon"> 
                <% if(data_count>0) { %>
              <a href='/report/society_ele_held_download?range_code=0&from_date=<%=from_date%>&to_date=<%=to_date%>&election_status=<%=ele_status%>&ctr_auth_id=<%=cntr_auth_id%>' ><img src="/images/excel.jpg" alt="excel" class="excel_cus" /></a>
                  <% } %>
            </div>
            </h4>
            <div class="table-responsive pt-3 custome_table_res">
              <div><b>All District</b></div>
            <br>
              <table class="table_cus table-striped" style="width:100%" id="example">
                <thead>
                  <tr>
                    <th class="society_th_1">SL No</th>
                    <th class="society_th_2">Society Name</th>
                    <th class="society_th_8">Society Type</th>
                    <th class="society_th_8">District</th>
                    <th class="society_th_8">Registration & Controlling Authority</th>
                    <th class="society_th_6">Reg.No.</th>
                    <th class="society_th_6">Reg.Date</th>
                    <th class="society_th_6">No. of members</th>
                    <th class="society_th_9">Last Election date</th>
                    <th class="society_th_9">Tenure Ends On</th>
                    <th class="society_th_9">Election Due Date</th>
                  </tr>
                </thead>
                <tbody id="item-list">
                  <% 
                  var i = 1; // Calculate starting index for page
                  data.forEach(dt => { %>
                      <tr>
                          <td><%= i++ %></td>
                          <td><%= dt.cop_soc_name %></td>
                          <td><%= dt.soc_type_name %></td>
                          <td><%= dt.dist_name %></td>
                          <td><%= dt.controlling_authority_type_name %></td>
                          <td><%= dt.reg_no %></td>
                          <td>
                            <%= 
                                (function() {
                                  let date = new Date(dt.reg_date);
                                  // If the date is invalid (NaN), return an empty string
                                  if (isNaN(date.getTime())) {
                                    return '';
                                  }
                                  // If the date is a valid Date object, format it
                                  return date.toLocaleDateString('en-IN', { year: 'numeric', month: 'short', day: 'numeric' });
                                })()
                              %>
                          </td>
                          <td><%= dt.num_of_memb %></td>
                          <td>
                            <%= 
                                (function() {
                                  let date = new Date(dt.last_elec_date);
                                  // If the date is invalid (NaN), return an empty string
                                  if (isNaN(date.getTime())) {
                                    return '';
                                  }
                                  // If the date is a valid Date object, format it
                                  return date.toLocaleDateString('en-IN', { year: 'numeric', month: 'short', day: 'numeric' });
                                })()
                              %>
                          </td>
                          <td>
                            <%= 
                                (function() {
                                  let date = new Date(dt.tenure_ends_on);
                                  // If the date is invalid (NaN), return an empty string
                                  if (isNaN(date.getTime())) {
                                    return '';
                                  }
                                  // If the date is a valid Date object, format it
                                  return date.toLocaleDateString('en-IN', { year: 'numeric', month: 'short', day: 'numeric' });
                                })()
                              %>
                           
                          </td>
                          <td>
                            <%= 
                                (function() {
                                  let date = new Date(dt.elec_due_date);
                                  // If the date is invalid (NaN), return an empty string
                                  if (isNaN(date.getTime())) {
                                    return '';
                                  }
                                  // If the date is a valid Date object, format it
                                  return date.toLocaleDateString('en-IN', { year: 'numeric', month: 'short', day: 'numeric' });
                                })()
                              %>
                          </td>
                      </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
        
          </div>
        </div>
      </div>
</div>


<style>
  #pagination a {
      padding: 5px 10px;
      margin: 0 2px;
      text-decoration: none;
      color: #007bff;
  }
  #pagination a.active {
      font-weight: bold;
      color: #000;
  }
  #pagination a:hover {
      text-decoration: underline;
  }
</style>


