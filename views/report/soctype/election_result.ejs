<style>
  #suggestions {
      position: absolute;
      z-index: 1000; /* Ensure it appears above other elements */
    
  }
</style>

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title card_title_cus">Categorywise(Society Type) Election Status Of Co-operative Societies <%= range_name %>
              
              <div class="table_title_icon"> 
              <a href='/report/dnlexcel_soctype?range=<%=range%>&soc_type_id=<%=soc_type%>' ><img src="/images/excel.jpg" alt="excel" class="excel_cus" /></a>
            </div>
            
            </h4>
            <div class="table-responsive pt-3 custome_table_res">
           
            <br>
              <table class="table_cus table-striped" style="width:100%">
                <thead>
                  <tr>
                    <th class="society_th_1">SL No</th>
                    <th class="society_th_2">Society Type </th>
                    <th class="society_th_2">Total </th>
                    <th class="society_th_6">Election Due</th>
                    <th class="society_th_6">Election Ongoing</th>
                    <th class="society_th_8">Election Held</th>
                  </tr>
                </thead>
                <tbody id="item-list">
                  <% 
                  var i = (page - 1) * 10 + 1; // Calculate starting index for page
                  var tot_tot = 0,tot_due = 0,tot_held = 0,tot_ongoing=0;
                  data.forEach(dt => {
                    tot_tot +=dt.total;
                    tot_due +=dt.DUE;
                    tot_ongoing +=dt.ONGOING;
                    tot_held += dt.HELD;
                    %>
                      <tr>
                          <td><%= i++ %></td>
                          <td><%= dt.soc_type_name %></td>
                          <td><%= dt.total %></td>
                          <td><%= dt.DUE %></td>
                          <td><%= dt.ONGOING %></td>
                          <td><%= dt.HELD %></td>
                          
                      </tr>
                  <% }) %>
                      <tr>
                        <th></th>
                        <th>Total</th>
                        <th><%= tot_tot %></th>
                        <th><%= tot_due %></th>
                        <th><%= tot_ongoing %></th>
                        <th><%= tot_held %></th>
                    </tr>
                </tbody>
              </table>
            </div>
        
          </div>
        </div>
      </div>
</div>


<style>
  #pagination a {
      padding: 5px 10px;
      margin: 0 2px;
      text-decoration: none;
      color: #007bff;
  }
  #pagination a.active {
      font-weight: bold;
      color: #000;
  }
  #pagination a:hover {
      text-decoration: underline;
  }
</style>


