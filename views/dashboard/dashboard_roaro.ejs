           var soc = response.soctot;
           $('#total_ongoing').html(soc.ongoing_tot ? soc.ongoing_tot : 0);
           $('#total_completed').html(soc.done_tot?soc.done_tot:0 );
           $('#total_due').html(soc.due_tot?soc.due_tot:0);
