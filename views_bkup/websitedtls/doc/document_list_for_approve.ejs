<style>
  #suggestions {
      position: absolute;
      z-index: 1000; /* Ensure it appears above other elements */
    
  }
</style>


<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title card_title_cus">
              <!-- <a href="/wdtls/addannouncement" class="btn btn-primary">Upload</a>  -->
              <div class="table_title_icon"> 
            </div>
            
            </h4>
            <div class="table-responsive pt-3 custome_table_res">
              <table class="table_cus table-striped" style="width:100%">
                <thead>
                  <tr>
                    <th class="society_th_1">SL No</th>
                    <th class="society_th_6">Document Title</th>
                    <th class="society_th_6">Document Type</th>
                    <th class="society_th_8">Document</th>
                    <th class="society_th_8">Document Status</th>
                    <th class="society_th_9">Approve</th>
                  </tr>
                </thead>
                <tbody id="item-list">
                  <% var i=1
                  data.forEach(dt => { %>
                      <tr>
                          <td><%= i++ %></td>
                          <td><%= dt.doc_title %></td>
                          <td><%= dt.doc_type_name %></td>
                          <td><% if (dt.status == 'A'){ %>
                            <span class="green">APPROVED</span>
                        <%  } else if(dt.status == 'U'){  %>
                          <span style="background-color: orangered;">UNAPPROVE</span>
                      <%  } %>
                          </td>
                          <td><%= dt.document %></td>
                          <td>
                            <% if (dt.status == 'U'){ %>
                            <a href="/wdtls/doc_for_approve?id=<%= dt.id %>" class="edit_dashboard">Approve</a>
                            <% } %>
                          </td>
                      </tr>
                  <% }) %>
                </tbody>
              </table>
        
            </div>
          </div>
        </div>
      </div>
</div>

<script>
   $(document).ready(function() {
                '<% if (success_msg.length > 0) { %>'
                    toastr.success('<%= success_msg %>');
           '<% } %>'
            });
  </script>