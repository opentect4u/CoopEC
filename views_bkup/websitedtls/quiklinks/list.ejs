<style>
  #suggestions {
      position: absolute;
      z-index: 1000; /* Ensure it appears above other elements */
    
  }
</style>


<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <div class="row card-title card_title_cus">
              <div class="col-md-9">Important Links    </div>
              <div class="col-md-3"><a href="/wdtls/addqlinks" class="btn btn-primary">add</a></div>
            </div>
            <!-- <h4 class="card-title card_title_cus">       
            </h4> -->
            <div class="table-responsive pt-3 custome_table_res">
            
              <table class="table_cus table-striped" style="width:100%">
              <!-- <table class="table table-striped"  id=""  style="width:100%"> -->
                <thead>
                  <tr>
                    <th class="society_th_1">SL No</th>
                    <th class="society_th_6">Title</th>
                    <th class="society_th_8">Link</th>
                    <th class="society_th_9">Option</th>
                  </tr>
                </thead>
                <tbody id="item-list">
                  <% var i=1
                  data.forEach(dt => { %>
                      <tr>
                          <td><%= i++ %></td>
                          <!-- <td><%= dt.doc_type %></td> -->
                          <td><%= dt.title %></td>
                          <td><%= dt.links %></td>
                          <td><a href="/wdtls/delqlinks?id=<%= dt.id %>" class="btn btn-primary btn-sm" onclick="confirmDelete(event)">Delete</a></td>
                          
                      </tr>
                  <% }) %>
                </tbody>
              </table>
        
            </div>

          
        
          </div>
        </div>
      </div>
</div>

